{
  "name": "br-register",
  "version": "1.0.0",
  "description": "Norwegian business register data collection and storage system",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "build:prod": "NODE_OPTIONS=--max-old-space-size=4096 tsc",
    "build:ec2": "NODE_OPTIONS=--max-old-space-size=8192 tsc",
    "start": "node dist/src/index.js",
    "fetch": "ts-node --transpile-only src/fetch-companies.ts",
    "sync": "ts-node --transpile-only src/sync-to-dynamodb.ts",
    "sync:pg": "ts-node --transpile-only src/sync-to-postgres.ts",
    "read:pg": "ts-node --transpile-only src/print-postgres-companies.ts",
    "read:annual-reports": "ts-node --transpile-only src/print-annual-reports.ts",
    "scrape:annual-reports": "ts-node --transpile-only src/scrape-annual-reports.ts",
    "cleanup:annual-reports": "ts-node --transpile-only src/cleanup-annual-reports.ts",
    "deploy": "cdk deploy",
    "synth": "cdk synth",
    "clean": "rm -rf dist cdk.out node_modules"
  },
  "keywords": [
    "brreg",
    "norway",
    "companies",
    "aws"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.0.379",
    "pdf2pic": "^3.2.0",
    "pg": "^8.16.3",
    "puppeteer": "^23.11.1",
    "stream-json": "^1.7.5",
    "tesseract.js": "^5.1.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/express": "^5.0.5",
    "@types/node": "^20.10.5",
    "@types/pdf-parse": "^1.1.3",
    "@types/pg": "^8.15.6",
    "@types/stream-json": "^1.7.8",
    "@vercel/node": "^3.0.0",
    "aws-cdk": "^2.120.0",
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
